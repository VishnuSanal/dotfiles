#!/bin/bash
bssid=$(nmcli device wifi list | sed -n '1!P'| cut -b 9- | rofi -dmenu | awk '{print $1}')
#pass=$(echo "" | dmenu -p "Enter password")
[ -z "$bssid" ] && exit 1
nmcli device wifi connect $bssid
notify-send "ðŸ“¶ WiFi Connected"

